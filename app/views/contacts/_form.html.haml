- form_for @contact, :html => {:multipart => true} do |f|
  = f.error_messages
  %p
    = f.label :name
    %br
    = f.text_field :name
  %p
    = f.label :email
    %br
    = f.text_field :email
  %p
    = f.label :photo
    %br
    = f.file_field :photo

  %p
    = f.label :description
    %br
    = f.text_area :description
  %p
    = f.label :phone
    %br
    = f.text_field :phone
  %p
    = f.submit "Submit"
